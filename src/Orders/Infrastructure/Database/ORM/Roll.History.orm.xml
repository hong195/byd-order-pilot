<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        https://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Orders\Domain\Aggregate\Roll\History" table="orders_roll_history">
        <id name="id" type="integer">
            <generator/>
        </id>

        <field name="rollId" type="integer" column="roll_id" nullable="true"/>

        <field name="employeeId" type="integer" column="employee_id" nullable="true"/>

        <field name="process" enum-type="App\Orders\Domain\ValueObject\Process" column="process" length="255"/>

        <field name="startedAt" type="datetime_immutable" column="started_at"/>

        <field name="finishedAt" type="datetime_immutable" column="finished_at" nullable="true"/>
    </entity>
</doctrine-mapping>